{
  "name": "params-cli",
  "version": "1.0.0",
  "description": "Easily extract CLI parameters for your Node application",
  "main": "build/index.js",
  "scripts": {
    "dev": "watch 'npm run build' src",
    "build": "babel src -d build --ignore '**/*.test.js'",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "keywords": [
    "cli",
    "arg",
    "arguments",
    "params"
  ],
  "author": "Hassen Taidirt <htaidirt@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.6.0",
    "watch": "^1.0.2"
  }
}
